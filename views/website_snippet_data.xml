<odoo>
    <!-- Register the snippet in website builder -->
    <template id="reseller_application_registration" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_structure']/div[@class='o_panel_body']" position="inside">
            <t t-snippet="reseller_application_snippet.s_reseller_application"
               t-thumbnail="/reseller_application_snippet/static/description/reseller.png"
               string="Reseller Application"/>
        </xpath>
    </template>
</odoo> 